<style>

  .featured-img-with-text-container{
    border-top:1px solid;
    border-bottom:1px solid;
/*     margin-top:100px; */
    display:flex;
    justify-content: center;
/*     margin-bottom: 10px; */
  }
  .featured-img-simple-text-block_svg {
    width: 20%;
        display: flex;
    justify-content: center;
    align-items: center;
}
  .inner-half-width svg {
    width: 50px;
    height: 50px;
    margin-bottom: 20px;
}
  .featured-img-simple-text-block_svg svg {
    margin: 0;
    padding: 0;
    width: 50px;
    height: 50px;
}
  .featured-img-simple-text-block_para p {
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 0;
    padding: 25px;
}
  .featured-img-simple-text-block_para {
    width: 80%;
        display: flex;
    justify-content: center;
    align-items: center;
}
  .featured-img-with-text-container>div{
    width:50%;
  }
  .featured-img-bnr-container img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
}
  .featured-img-inner-container{
      margin: 0px 45px;
  } 
  .featured-img-heading-container{{section.id}}{
    color:{{section.settings.main_head_clr}};
  }
   .featured-img-heading-container{{section.id}} span{
    color:{{section.settings.main_head_clr_}};
  }
  
.featured-img-inner-container h1 {
    margin: 40px 0px;
    font-style: normal;
    font-weight: 500;
    font-size: 60px;
    line-height: 130.5%;
    text-transform: capitalize;
}
  .flex_div{
    display:flex !important;
  }
  .flex_div>div{
    width:48%;
  }
  .featured-img-simple-text-with-img-block-one , .featured-img-simple-text-with-img-block-two , .featured-img-simple-text-with-img-block-three{
    display:flex;
  }
  
  
  .half-width{
    width:50%;
   
  }
  .featured-text-container{
    border-left:1px solid;
  }
  .featured-text-inner-container>div{
    border-bottom:1px solid;
  }
  .featured-img-simple-text-block{
    display: flex;
  }
  .custom-svg{
    width:15%;
    padding-top: 12px;
    margin-top: 24px;
  }
  .custom-p{
    width:100%;
    padding-top: 25px;
  }
  .half-width-img{
    display: flex;
    justify-content: center;
    flex-direction: column;
  }
    .inner-half-width{
      padding:35px 30px;
    }


  .relative-div-one {
    position:relative;
  }
  .relative-div-two {
    position:relative;
    border-left: 1px solid;
  }
  .relative-div-three {
    position:relative;
   
    padding:30px;
  }
  .border-svg{
        position: absolute;
    top: -15px;
    right: -15px;
    width: 30px;
    height: 30px;

  }
  .border-svg-one {
    position: absolute;
    bottom: -15px;
    right: -15px;
    z-index: 10;
    width: 30px;
    height: 30px;
}
  .border-svg-two{
    position: absolute;
    bottom: -15px;
    left: -15px;
    width: 30px;
    height: 30px;
  }
  .border-svg-three {
    position: absolute;
    bottom: -15px;
    right: -15px;
    width: 30px;
    z-index: 5;
    height: 30px;
}
  .half-width-right{
   
  }
  .half-width-left{
    border-left: 1px solid;
  }
   .half-width-left2{
      border-left: 1px solid black;
    }
  .mobile-svg{
    position:absolute;
    display:none;
  }
  .mobile-svg-two{
      display:none;
      position:absolute;
  } 
  .mobile-svg-one{
      display:none;
      position:absolute;
    }
  .red{
    color:#E11F65;
  }
  .common_p span{
    
  }



  @media screen and (max-width: 768px){
    .featured-img-with-text-container{
      flex-direction:column;
      border-bottom:0px;
    }
    .featured-img-heading-container{{section.id}}{
      display: flex;
    flex-direction: column;
    width: 100%;
    }
