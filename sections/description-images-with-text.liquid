    border-left:0px solid;
    }
    .featured-text-inner-container>div{
    border-bottom:0px solid;
    }
    .half-width-right{
    border-right:0px solid;
  }
    .half-width-left{
    border-left: none;
  }
  
    .half-width-left2{
      border-left:none;
    }
    .relative-div-three {
    border-right:0px solid;
  }
