        "3a9ac501-2ffc-4029-b55a-3f0f81d44c83": {
          "type": "slide",
          "settings": {
            "richtext1": "<p>Marie A., Wilmette, IL, USA<\/p>",
            "richtext2": "<p>\"It is so comfortable. I would buy another for my friends who are nurses. I surely will recommend it to them\"<\/p>",
            "range": 5
          }
        },
        "2d68b0bd-d642-4f6a-91e5-5c1f58f4f575": {
          "type": "slide",
          "settings": {
            "richtext1": "<p>Mae G., Miami, FL, USA<\/p>",
            "richtext2": "<p>\"The brand is more than shoes it is made of love and comfort. The minimalist design could match whether you go to work or run to the grocery. It's a statement... work + style !<\/p>",
            "range": 5
          }
        }
